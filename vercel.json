{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/api/auth/.*", "dest": "/src/routes/auth/authRoutes.js" },
    { "src": "/api/.*", "dest": "/src/routes/mainRoutes.js" },
    { "src": "/.*", "dest": "/src/index.js" }
  ]
}
